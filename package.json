{
  "name": "reason-react-native-maps",
  "version": "0.27.0",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react-native-maps": "~0.27.0"
  },
  "author": "sgny (https://github.com/sgny)",
  "repository": "https://github.com/sgny/reason-react-native-maps.git",
  "license": "MIT",
  "keywords": [
    "reason",
    "reasonml",
    "bucklescript",
    "react-native",
    "react-native-maps"
  ],
  "files": [
    "*",
    "!.DS_Store",
    "!**/*.bs.js",
    "!.merlin",
    "!lib/bs",
    "!lib/ocaml",
    "!.gitignore",
    "!**/debug.log",
    "!**/build.yml"
  ],
  "scripts": {
    "format:most": "prettier --write \"**/*.{md,json,js,css}\"",
    "format:re": "find . -name \"*.re\" -or -name \"*.rei\" | grep -v \"node_modules\" | xargs bsrefmt --in-place",
    "format": "yarn format:most && yarn format:re",
    "re:start": "bsb -make-world -w",
    "re:build": "bsb -make-world",
    "re:clean-build": "bsb -clean-world -make-world",
    "start": "yarn re:start",
    "build": "yarn re:build",
    "test": "yarn re:clean-build"
  },
  "devDependencies": {
    "bs-platform": "^8.2.0",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.18.0",
    "reason-react": "^0.9.1",
    "reason-react-native": "^0.62.3"
  },
  "prettier": {
    "trailingComma": "all",
    "proseWrap": "always"
  },
  "lint-staged": {
    "*.{md,json,js,css}": [
      "prettier --write",
      "git add"
    ],
    "*.{re,rei}": [
      "bsrefmt --in-place",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
